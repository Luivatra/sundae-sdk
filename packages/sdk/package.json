{
  "name": "@sundae/sdk",
  "version": "0.1.0",
  "description": "The official SundaeSwap protocol SDK for clients.",
  "repository": "git@github.com:SundaeSwap-finance/sundae-sdk.git",
  "author": "Calvin Koepke <calvin@sundaeswap.finance>",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "yarn lint -- --fix",
    "lint-staged": "lint-staged",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --watchAll=false",
    "clean": "rm -rf ./dist",
    "typecheck": "tsc --noEmit",
    "compile:esm": "tsc --outDir dist/esm",
    "compile:cjs": "tsc --outDir dist/cjs --module commonjs",
    "compile:types": "tsc --outDir dist/types --emitDeclarationOnly --declarationMap --declaration",
    "compile": "yarn compile:cjs && yarn compile:esm && yarn compile:types",
    "build": "yarn clean && yarn compile",
    "watch": "yarn compile:cjs && yarn compile:types && yarn compile:esm --watch"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "@martifylabs/mesh": "^1.2.0-beta.4"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@sundae/eslint-config": "^1.6.2",
    "@sundae/jest-config": "^1.3.1",
    "@sundae/react-hooks": "^1.7.1",
    "@sundae/tailwind-config": "^1.7.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/jest": "^28.1.3",
    "@types/react": "^18.0.8",
    "@types/react-table": "^7.7.12",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^29.2.2",
    "babel-loader": "^8.2.5",
    "buffer": "^6.0.3",
    "husky": "^7.0.0",
    "jest-dom": "^4.0.0",
    "lint-staged": "^12.4.1",
    "postcss": "^8",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "tailwindcss": "^3.0.24",
    "tailwindcss-animation-delay": "^1.0.7",
    "tailwindcss-radix": "^2.5.0",
    "typescript": "^4.6.4",
    "webpack": "^5.74.0"
  }
}
