{
  "name": "@sundaeswap/sdk",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "lerna bootstrap",
    "test": "lerna run test --verbose",
    "prepare": "husky install",
    "build": "lerna run build",
    "version:ci": "lerna version --conventional-commits --create-release github -y",
    "publish:ci": "lerna publish from-package -y --ignore-scripts",
    "docs": "lerna run docs:ci"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@meshsdk/core": "^1.3.0",
    "@semantic-release/git": "^10.0.1",
    "@sundaeswap/docgen": "^0.1.1",
    "@types/node": "^18.11.18",
    "eslint": "^8.31.0",
    "glob": "^8.0.3",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "jest-dom": "^4.0.0",
    "lerna": "^5.6.2",
    "lucid-cardano": "^0.8.4",
    "ts-jest": "^29.0.4",
    "ts-node": "^10.9.1",
    "typedoc-plugin-markdown": "^3.14.0"
  },
  "dependencies": {}
}
