{
  "name": "@sundaeswap/sdk",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "lerna bootstrap",
    "test": "lerna run test",
    "prepare": "husky install",
    "build": "lerna run build",
    "release": "yarn build && yarn semantic-release && lerna publish from-package -y --no-private",
    "docs": "lerna run yarn docs:ci",
    "semantic-release": "semantic-release --dry-run"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@meshsdk/core": "^1.3.0",
    "@microsoft/api-documenter": "^7.19.27",
    "@microsoft/api-extractor": "^7.33.7",
    "@redocly/cli": "^1.0.0-beta.120",
    "@sundaeswap/docgen": "^0.1.1",
    "@types/node": "^18.11.18",
    "glob": "^8.0.3",
    "lerna": "^5.6.2",
    "lucid-cardano": "^0.8.4",
    "semantic-release": "^20.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typeconv": "^1.8.0",
    "typedoc-plugin-markdown": "^3.14.0"
  },
  "dependencies": {}
}
